{
  "name": "ory/hydra-client",
  "description": "Welcome to the ORY Hydra HTTP API documentation. You will find documentation for all HTTP APIs here.",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/ory/hydra-client-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Ory GmbH",
      "homepage": "https://www.ory.sh"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "Ory\\Hydra\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ory\\Hydra\\Client\\Test\\": "test/"
    }
  }
}
