{
  "name": "ory/client",
  "description": "Documentation for all public and administrative Ory APIs. Administrative APIs can only be accessed with a valid Personal Access Token. Public APIs are mostly used in browsers.",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/ory/client-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Ory GmbH",
      "homepage": "https://www.ory.sh"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "Ory\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ory\\Client\\Test\\": "test/"
    }
  }
}
